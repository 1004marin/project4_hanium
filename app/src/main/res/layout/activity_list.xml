<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f6f6"
    tools:context=".RegisterCompleteActivity">

    <View
        android:id="@+id/view4"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:background="#f2f2f2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">
    </View>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="열려라 해물!"
        android:textColor="#654b8d"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.484"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="100dp"
        android:text="직원 목록"
        android:textColor="#4a4a4a"
        android:textSize="21sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/list_emp_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list_emp_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="직원 이름"
        android:textColor="#5b5b5b"
        android:textSize="8sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/list_emp_name_content"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/list"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="127dp"
        android:layout_height="94dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="244dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="333dp"
        android:src="@drawable/face"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/list_emp_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list_emp_name_content"
        android:layout_width="82dp"
        android:layout_height="20dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="16dp"
        android:background="#efefef"
        android:gravity="center_vertical"
        android:text="쿵야"
        android:textColor="#5b5b5b"
        android:textSize="11sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/list_emp_phone"
        app:layout_constraintEnd_toStartOf="@+id/list_emp_dept_content"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/list_emp_name"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list_emp_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="소속 부서"
        android:textColor="#5b5b5b"
        android:textSize="8sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/list_emp_phone_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/list_emp_name_content"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list_emp_phone_content"
        android:layout_width="82dp"
        android:layout_height="20dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="333dp"
        android:background="#efefef"
        android:gravity="center_vertical"
        android:text="쿵야"
        android:textColor="#5b5b5b"
        android:textSize="11sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/list_emp_phone"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list_emp_dept"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="246dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="6dp"
        android:text="휴대 번호"
        android:textColor="#5b5b5b"
        android:textSize="8sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/list_emp_dept_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/list_emp_name"
        app:layout_constraintTop_toBottomOf="@+id/view4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/list_emp_dept_content"
        android:layout_width="82dp"
        android:layout_height="20dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="388dp"
        android:background="#efefef"
        android:gravity="center_vertical"
        android:text="쿵야"
        android:textColor="#5b5b5b"
        android:textSize="11sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_emp_dept"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>